{
    "name": "react-leaflet-textpath",
    "version": "1.0.5",
    "description": "React wrapper of leaflet-textpath",
    "main": "dist/index.js",
    "module": "src/index.js",
    "scripts": {
        "test": "jest",
        "cover": "jest --coverage --collectCoverageFrom=src/**/*.js",
        "lint": "eslint ./src/**/*.js ./__tests__/**/*.js",
        "changelog": "auto-changelog -p --tag-prefix v",
        "release": "np",
        "version": "npm run changelog && git add CHANGELOG.md",
        "build": "babel src --out-dir dist",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clementallen/react-leaflet-textpath.git"
    },
    "author": "Clement Allen (https://clementallen.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/clementallen/react-leaflet-textpath/issues"
    },
    "homepage": "https://github.com/clementallen/react-leaflet-textpath#readme",
    "keywords": [
        "react",
        "leaflet",
        "react-leaflet",
        "leaflet-textpath",
        "react-leaflet-textpath"
    ],
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "auto-changelog": "^1.12.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.7.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "eslint": "^5.16.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "jest": "^24.7.1",
        "leaflet": "^1.4.0",
        "np": "^4.0.2",
        "prettier": "^1.16.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-leaflet": "^2.2.1"
    },
    "peerDependencies": {
        "leaflet": "^1.0.0",
        "react": "^15.0.0 || ^16.0.0",
        "react-dom": "^15.0.0 || ^16.0.0",
        "react-leaflet": "^2.0.0"
    },
    "dependencies": {
        "leaflet-textpath": "^1.2.0"
    }
