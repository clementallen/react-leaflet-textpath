{
    "name": "react-leaflet-textpath",
    "version": "2.0.0",
    "description": "React wrapper of leaflet-textpath",
    "main": "dist/index.js",
    "module": "src/index.ts",
    "types": "types/index.d.ts",
    "scripts": {
        "test": "jest",
        "cover": "jest --coverage --collectCoverageFrom=src/**/*.js",
        "lint": "eslint ./src/**/*.js ./__tests__/**/*.js",
        "type-check": "tsc --pretty --noEmit",
        "changelog": "auto-changelog -p --tag-prefix v",
        "release": "np",
        "version": "npm run changelog && git add CHANGELOG.md",
        "build": "npm run build:js && npm run build:types",
        "build:js": "babel src --extensions \".ts\" --out-dir dist",
        "build:types": "tsc --emitDeclarationOnly",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clementallen/react-leaflet-textpath.git"
    },
    "author": "Clement Allen (https://clementallen.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/clementallen/react-leaflet-textpath/issues"
    },
    "homepage": "https://github.com/clementallen/react-leaflet-textpath#readme",
    "keywords": [
        "react",
        "leaflet",
        "react-leaflet",
        "react-leaflet-v3",
        "leaflet-textpath",
        "react-leaflet-textpath"
    ],
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.13.0",
        "@types/leaflet-textpath": "^1.2.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
        "auto-changelog": "^2.2.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "enzyme": "^3.11.0",
        "eslint": "^7.18.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "jest": "^26.6.3",
        "leaflet": "^1.7.1",
        "np": "^7.2.0",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-leaflet": "^3.0.0",
        "typescript": "^4.2.3"
    },
    "peerDependencies": {
        "leaflet": "^1.0.0",
        "react": "^15.0.0 || ^16.0.0 || ^17.0.0",
        "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0",
        "react-leaflet": "^3.0.0"
    },
    "dependencies": {
        "leaflet-textpath": "^1.2.3"
    }
}
